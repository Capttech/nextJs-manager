// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model userVehicles {
  userId    String
  plate     String   @id
  model     String
  color     String
  year      Int
  millage   Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model userHouses {
  userId    String
  address   String   @id
  type      String
  salePrice Int
  garage    Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model userOther {
  userId    String
  name      String   @id
  type      String
  state     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
